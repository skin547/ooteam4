<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>CategoryServlet</servlet-name>
    <servlet-class>main.java.controller.servlet.CategoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CategoryServlet</servlet-name>
    <url-pattern>/categoryServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PropertyServlet</servlet-name>
    <servlet-class>main.java.controller.servlet.PropertyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PropertyServlet</servlet-name>
    <url-pattern>/propertyServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ProductServlet</servlet-name>
    <servlet-class>main.java.controller.servlet.ProductServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProductServlet</servlet-name>
    <url-pattern>/productServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ProductImageServlet</servlet-name>
    <servlet-class>main.java.controller.servlet.ProductImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProductImageServlet</servlet-name>
    <url-pattern>/productImageServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>BackServletFilter</filter-name>
    <filter-class>main.java.controller.filter.BackServletFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>BackServletFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>